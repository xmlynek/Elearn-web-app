{
  "name": "nodejs-backend",
  "version": "1.0.0",
  "description": "Nodejs backend for web E-learning app",
  "author": "Filip Ml√Ωnek <mlynekff@gmail.com>",
  "license": "MIT",
  "keywords": [
    "REST API",
    "Node",
    "TypeScript",
    "Express",
    "Prisma",
    "Backend"
  ],
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node --transpile-only src/server.ts",
    "dev": "nodemon src/server.ts",
    "build": "tsc && cd client && npm install && npm run build",
    "prisma:migrate": "dotenv -e config.env -- npx prisma migrate dev --name prisma-init",
    "prisma:push": "dotenv -e config.env -- npx prisma db push --preview-feature",
    "debug": "ndb src/server.ts",
    "predocker:build": "cd client && npm install && npm run build",
    "docker:build": "docker build -f Dockerfile -t xmlynek/react-node-elearn-app:latest -t xmlynek/react-node-elearn-app:%npm_package_version% .",
    "docker:push": "docker push --all-tags xmlynek/react-node-elearn-app",
    "docker-compose:start": "docker-compose up",
    "docker-compose:start-d": "docker-compose up -d",
    "docker-compose:down": "docker-compose down"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "express-rate-limit": "^6.4.0",
    "http-status": "^1.5.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.16",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "swagger-jsdoc": "^6.2.4",
    "swagger-ui-express": "^4.5.0",
    "winston": "^3.7.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "^2.6.1",
    "prisma": "^3.15.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
